{
  "name": "@panhezeng/before-run-webpack-plugin",
  "description": "before-run-webpack-plugin",
  "version": "1.0.7",
  "author": "潘何增 <panhezeng@gmail.com> (http://apsay.com/)",
  "license": "MIT",
  "private": false,
  "keywords": [
    "before compiler run",
    "webpack plugin",
    "javascript"
  ],
  "homepage": "https://github.com/panhezeng/before-run-webpack-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panhezeng/before-run-webpack-plugin.git"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "dist/before-run-webpack-plugin.min.js",
  "scripts": {
    "build": "webpack --mode production"
  },
  "dependencies": {
    "shelljs": "latest"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md,jsx,vue,ts,less}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**",
      "**/docs/**"
    ]
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
